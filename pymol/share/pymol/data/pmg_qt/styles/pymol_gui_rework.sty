/* PYMOL 2.0 DARKSTYLE */


/* Main window */
#extgui,
QMenuBar,
QMenuBar::item /* required with Win10+PyQt4 */
{
    background:     #343434;
    color:          #E8E8E8;
}

QMenuBar::item:selected {
    background:     #ffffff;
    color:          #000000;
}

QLineEdit#command_line
{
    background-color: #3E3E3E;
    border:         1px solid #494949;
    color:          #E8E8E8;
    height:         20px;
}

QLabel#command_label
{
    color: #9b81fd;
}

QLabel
