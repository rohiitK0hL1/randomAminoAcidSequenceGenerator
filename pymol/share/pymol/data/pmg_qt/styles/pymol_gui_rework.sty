{
    background-color:   #494949;
    border-radius:      5px;
}

QSlider::groove#global_frame_controls_slider
{
    height:         4px;
    border-radius:  2px;
    background:     #4C4C4C;
    margin:         10px 10px;
}

QSlider::handle#global_frame_controls_slider
{
    height:         10px;
    width:          15px;
    border:         1px solid #505050;
    border-radius:  7.5px;
    margin-left:    -7.5px;
    margin-right:   -7.5px;
    margin-top:     -7.5px;
    margin-bottom:  -7.5px;
    background:     #BCBCBC;
}

/* Toggle Panels Menu */
TogglePanelsMenu
{
    background:     #343434;
    border:         1px solid #494949;
    border-radius:  5px;
}

TogglePanelButton
{
    font-weight:    normal;
    Text-align:     left;
    color:          #cecece;
    background:     #383838;
    padding-left:   25px;
    padding-right:  5px;
    padding-top:    5px;
    padding-bottom: 5px;
}

TogglePanelButton:hover
{
    background-color:#4D4D4D;
}

QWidget#horizontal_divider
{
    background-color:   #494949;
}

/* Selection Mode Menu */
SelectionModeOptionButton[active=false]
{
    font-weight:    normal;
    font-size:      13px;
    Text-align:     left;
    color:          #cecece;
    background:     #383838;
    padding-left:   5px;
}

SelectionModeOptionButton[active=true]
{
    font-weight:    normal;
    font-size:      13px;
    Text-align:     left;
    color:          #7F7AFF;
    background:     #383838;
    padding-left:   5px;
}

SelectionModeOptionButton[disabled=true]
{
    font-weight:    normal;
    font-size:      13px;
    Text-align:     left;
    color:          gray;
    background:     #383838;
    padding-left:   5px;
}

SelectionModeOptionButton[hover=true]
{
    background-color:#4D4D4D;
}

SelectionModeOptionButton:disabled
{
    background:     #383838;
}

QLabel#selection_mode_label
{
    font-weight:    normal;
    font-size:      12px;
    color:          #cecece;
    background:     #494949;
    border-radius:  5px;
    padding-top:    2.5px;
    padding-bottom: 2.5px;
}

/* Scene Tile */
SceneTile
{
    border-radius:  5px;
    margin:         0px 9px;
}

SceneTile[selected=false]
{
    border:         1px solid #494949;
}

SceneTile[selected=true]
{
    border:         2px solid #767676;
}

SceneTileMenu
{
    background:     #434343;
}

PlaceholderSceneTile
{
    margin:         0px 9px;
    color:          #767676;
    font-size:      30px;
    border-radius:  5px;
    border:         2px solid #767676;
}

/* Input Dialog */
InputDialog
{
    background:     #343434;
}

InputDialogLabel
{
    font-weight:    normal;
    color:          #cecece;
    background:     #343434;
}

InputDialogButton
{
    background:     #333333;
    border:         1px solid #5A5A5A;
    border-radius:  3px;
    color:          #cecece;
    padding:        5px 20px;
}

InputDialogButton:hover
{
    background:     #4D4D4D;
}

InputDialogCheckBox
{
    font-weight:    normal;
    color:          #cecece;
}

InputDialogComboBox
{
    border:         1px solid #494949;
    border-radius:  3px;
    background:     #252525;
    color:          #cecece;
}

QWidget #input_dialog_combo_box_frame
{
    border:         1px solid #494949;
    border-radius:  3px;
    background:     #343434;
    color:          #cecece;
    margin:         0px;
    padding:        0px;
}

